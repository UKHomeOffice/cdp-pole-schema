  "version": "0.11",
  "additionalProperties": false,
  "type": "string",
  "enum": [
    "Matched",
    "Travelled",
    "Travelled with",
    "Depart from",
    "Arrive at",
    "Embark at",
    "Disembark at",
    "Booked",
    "Booked on",
    "Booking for",
    "Booking message",
    "On",
    "Owns",
    "Holds",
    "Identifies",
    "DCS drop",
    "API drop",
    "PNR drop",
    "Journey with",
    "Leg from",
    "Leg to",
    "Leg"
